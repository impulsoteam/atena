{
  "name": "atena",
  "version": "0.2.1",
  "description": "app to integrate slack with analytics",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://github.com/impulsonetwork/atena#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/impulsonetwork/atena"
  },
  "bugs": {
    "url": "https://github.com/impulsonetwork/atena/issues"
  },
  "keywords": [
    "Atena",
    "Impulso",
    "Gamification",
    "Slack",
    "Analytics"
  ],
  "scripts": {
    "format": "prettier --write \"**/*.js\"",
    "start": "babel-node index.js",
    "start:prod": "babel-node index.js",
    "start:dev": "npm run format && set NODE_ENV=development && nodemon --exec babel-node index.js",
    "start:dev:debug": "npm run format && set NODE_ENV=development && nodemon --exec babel-node --inspect-brk index.js",
    "lint": "npm run format && eslint \"**/*.js\" --ignore-pattern node_modules/ --ignore-pattern coverage/",
    "test:coverage": "set NODE_ENV=test && jest --coverage --forceExit --detectOpenHandles",
    "test": "set NODE_ENV=test  && jest --forceExit --detectOpenHandles",
    "test:watch": "set NODE_ENV=test && jest --watch --onlyChanged --forceExit --detectOpenHandles",
    "lint-md": "remark ."
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "dependencies": {
    "@rebass/grid": "^6.0.0-7",
    "@rocket.chat/sdk": "^0.2.9",
    "@slack/client": "^4.6.0",
    "@slack/events-api": "^2.1.0",
    "ajv": "6.5.3",
    "amqplib": "^0.5.3",
    "axios": "^0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "config-yml": "^0.10.2",
    "dotenv": "^6.0.0",
    "eslint": "^5.6.0",
    "eslint-plugin-prettier": "3.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "filereader": "^0.10.3",
    "fs": "^0.0.1-security",
    "global": "^4.3.2",
    "googleapis": "34.0.0",
    "if-env": "^1.0.4",
    "mime-types": "^2.1.21",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "mongoose": "^5.2.14",
    "morgan": "^1.9.0",
    "node-cron": "^2.0.3",
    "passport": "^0.4.0",
    "passport-linkedin-oauth2": "^1.6.1",
    "path": "^0.12.7",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "querystring": "^0.2.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "readline": "^1.3.0",
    "styled-components": "^4.1.3",
    "styled-normalize": "^8.0.4",
    "universal-analytics": "^0.4.17"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-module-resolver": "^3.1.3",
    "eslint-plugin-react": "^7.12.4",
    "factory-girl": "^5.0.4",
    "ghooks": "^2.0.4",
    "jest": "^24.1.0",
    "mongodb-memory-server": "^3.1.2",
    "nodemon": "^1.18.7",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-recommended": "^3.0.2",
    "sinon": "^7.1.3",
    "sinon-express-mock": "^2.1.0",
    "sinon-mongoose": "^2.2.1",
    "supertest": "^3.3.0"
  }
}
